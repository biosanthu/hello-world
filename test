 Map<Id,Lead> ldMap = new Map<Id,Lead>([SELECT id,Name,Phone,Email,FormName__c,LeadRegisteredDateTime__c,DealerName__c,LeadSource,Owner.name,LMS_CarModel__c FROM lead WHERE  Phone IN (\''+String.join(Custid,'\',\'')+'\')')]);
 
 System.debug(' Size : ' + ldMap.size());
